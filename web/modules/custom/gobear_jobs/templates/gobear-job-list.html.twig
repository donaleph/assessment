{% if items %}
<ul class="list-unstyled gobear-job-list">
  {% for item in items %}
    <li class="clearfix">
      <div>
        {{ item.title }}
        {{ item.company }}
        {{ item.location }}
        {{ item.created_at }}
      </div>
    </li>
  {% endfor %}
</ul>
{% else %}
  <p>
    {% trans %}
      No items found. Please check again later.
    {% endtrans %}
  </p>
{% endif %}

